<?xml version='1.0' encoding='utf-8'?>
<widget id="app pesco" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>app pesco</name>
    <description>
        app pesco
    </description>
    <author email="eric.birkner@simplechile.com" href="http://simplechile.com">
        Simple Chile
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" launch-external="true" />
    <allow-intent href="https://*/*" launch-external="true" />
    <allow-intent href="tel:*" launch-external="true" />
    <allow-intent href="sms:*" launch-external="true" />
    <allow-intent href="mailto:*" launch-external="true" />
    <allow-intent href="geo:*" launch-external="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
